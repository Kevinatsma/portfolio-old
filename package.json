{
  "name": "portfolio-kevin",
  "version": "1.0.0",
  "private": true,
  "description": "Example project from an angular.io guide.",
  "scripts": {
    "ng": "ng",
    "nghm": "node --max_old_space_size=8096 ./node_modules/@angular/cli/bin/ng",
    "build": "ng build",
    "start": "ng serve",
    "test": "ng test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "dev:ssr": "ng run portfolio:serve-ssr",
    "build:ssr": "ng build --prod && gzip-all \"dist/browser/*.*\" && ng run portfolio:server:production",
    "serve:ssr": "node dist/server/main.js",
    "prerender": "nghm run portfolio:prerender",
    "build:client-and-server-bundles": "npm run nghm build --prod && gzip-all \"dist/browser/*.*\" && npm run nghm run portfolio:server:production --bundleDependencies true",
    "dev": "npm run build:ssr && npm run serve:ssr"
  },
